#include<bits/stdc++.h>
using namespace std;
 
int main()
{
    int i,cnt=0;
    char arr[20];
    scanf("%s",arr);
    for(i=0;i<strlen(arr);i++){
        if(arr[i]=='4'||arr[i]=='7'){
            cnt++;
        }
    }
    if(cnt==4||cnt==7){
        printf("YES\n");
    }
    else
        printf("NO\n");
 
    return 0;
}
