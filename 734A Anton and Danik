#include<bits/stdc++.h>
using namespace std;
 
 int main()
 {
     int n;
     int l1=0,l2=0;
     cin>>n;
     string s;
     cin>>s;
     for(int i=0;i<n;i++)
     {
         if(s[i]=='A')
            l1++;
         if(s[i]=='D')
         l2++;
 
     }
     if(l1>l2) cout<<"Anton"<<endl;
     else if(l1<l2) cout<<"Danik"<<endl;
     else if(l1==l2) cout<<"Friendship"<<endl;
 
 
 }
