#include<bits/stdc++.h>
using namespace std;
 
int main()
 
 {
     string w="hello";
   string s;
     int p=0;
     cin>>s;
    int l=s.length();
     int a=0;
     int cnt=0;
     for(int i=0;i<l;i++){
        if(s[i]==w[a]){
            a++;
            cnt++;
        }
     }
     if(cnt==5) printf("YES\n");
     else printf("NO\n");
 
     return 0;
 }
 
